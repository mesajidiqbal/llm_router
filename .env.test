# Environment
ENV=test

# Always use mock providers for tests
MOCK=1

# Test API keys (not used in mock mode but required for settings validation)
OPENAI_API_KEY=test-openai-key
OPENAI_API_MODEL=gpt-5
GOOGLE_API_KEY=test-google-key
GOOGLE_API_MODEL=gemini-3-pro

# No random failures in tests
MOCK_FAILURE_RATE=0.0

# Circuit breaker settings for tests
CIRCUIT_BREAKER_FAILURE_THRESHOLD=3
CIRCUIT_BREAKER_OPEN_DURATION_S=60

# Routing strategy settings
STRATEGY_QUALITY_BOOST=1.1
STRATEGY_COST_SPEED_BOOST=0.9
